{
  "printWidth": 100,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "arrowParens": "always",
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "semi": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "tabWidth": 3,
  "importOrder": [
    ".*styles.css$",
    "",
    "^react$",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@docs/(.*)$",
    "^@/.*$",
    "^../(?!.*.css$).*$",
    "^./(?!.*.css$).*$",
    "\\.css$"
  ],
  "overrides": [
    {
      "files": "*.mdx",
      "options": {
        "printWidth": 70
      }
    }
  ]
}
